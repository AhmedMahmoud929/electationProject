<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Dashboard</title>
    <!-- Font Awesome -->
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.2/css/all.min.css"
      integrity="sha512-z3gLpd7yknf1YoNbCzqRKc4qyor8gaKU1qmn+CShxbuBusANI9QpRohGBreCFkKxLhei6S9CQXFEbbKuqLg0DA=="
      crossorigin="anonymous"
      referrerpolicy="no-referrer"
    />
    <!-- Main Styles -->
    <link rel="stylesheet" href="../css/index.css" />
    <link rel="stylesheet" href="../css/upload.css" />
    <link rel="stylesheet" href="../css/form.css" />
    <link rel="stylesheet" href="../css/table.css" />
  </head>
  <body>
    <!-- HEADER -->
    <header class="header">
      <div class="header-content responsive-wrapper">
        <div class="header-logo">
          <a href="/">
            <div>
              <img
                src="https://assets.codepen.io/285131/untitled-ui-icon.svg"
              />
            </div>
            <img src="https://assets.codepen.io/285131/untitled-ui.svg" />
          </a>
        </div>
        <i class="fa-solid fa-bars"></i>
        <a href="/logout" class="button danger">
          <span>Logout</span>
        </a>
      </div>
    </header>
    <!-- MAIN -->
    <main class="main">
      <div class="responsive-wrapper">
        <div class="content">
          <%- include("../parts/aside.ejs", {active:"upload-electors"}) %>
          <div class="content-main">
            <div class="view-group">
              <div class="feedback-overview">
                <h1><span><%= allCount-failedCount %></span>/<%= allCount%></h1>
                <p>elector are uploaded sucessfully</p>
              </div>
              <% if(failedRows.length){ %>
              <div class="invalid-lines">
                <h1>Invalid lines</h1>
                <ul>
                  <% failedRows.forEach(row=> { %>
                  <li>
                    Line <%= row.line %> :
                    <br />
                    <pre>
 <% Object.keys(row).forEach((key, index) => { %><%= key %>:<%=JSON.stringify(row[key]) %><% if (index <
Object.keys(row).length - 1) { %>,
<% } %> <% }) %></pre
                    >
                  </li>
                  <% })%>
                </ul>
              </div>
            </div>
            <% } %>
          </div>
        </div>
      </div>
    </main>

    <!-- Main Scripts -->
    <script src="../js/asideToggle.js"></script>
    <script src="../js/checkboxToggle.js"></script>
  </body>
</html>
